{
  "name": "GoodCoin",
  "version": "1.0.0",
  "description": "GoodCoin PoC",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test --inspect-brk  --compilers js:babel-core/register test/GoodCoin.js",
    "start:node": "start node app/server.js",
    "migrate:ganache":"truffle migrate --network ganache_develop",
    "migrate_reset:ganache":"truffle migrate --network ganache_develop --reset",
    "start:dev": "start http://localhost:3001/app/index.html &webpack-dev-server --mode development  --config ./app/webpack.config.js --port 3001 --hot --inline --inspect=9001",
    "build:dev": "webpack --mode development ./app/src/index.js --output ./app/dist/bundle.js",
    "build:prd": "webpack --mode production ./app/src/index.js --output ./app/dist/bundle.js",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "http-server": "^0.11.1",
    "jquery": "^3.3.1",
    "npm": "^6.4.1",
    "truffle-core": "^4.1.14",
    "truffle-hdwallet-provider": "0.0.3",
    "web3": "^1.0.0-beta.36",
    "zeppelin-solidity": "^1.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "fs-extra": "^6.0.1",
    "style-loader": "^0.23.0",
    "truffle": "4.1.11",
    "truffle-privatekey-provider": "^0.1.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.8"
  }
}
